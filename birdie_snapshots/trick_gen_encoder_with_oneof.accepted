pub fn encode_response(response: Response) {
  encode.message([case response.result {
        Some(oneof_value) -> case oneof_value {
          Success(value) -> encode.string_field(1, value)
          Error(value) -> encode.string_field(2, value)
        }
        None -> <<>>
      }])
}


