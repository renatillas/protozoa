pub fn encode_team(team: Team) {
  {
    let members_fields =
      list.map(team.members, fn(v: String) { encode.string_field(2, v) })
    encode.message(
      list.flatten([members_fields, [encode.string_field(1, team.name)]]),
    )
  }
}


